{
    "name": "Code Owners - Notifier",
    "description": "Extra features to ASAC virtual campus",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "16": "./logo16.png",
        "32": "./logo32.png",
        "64": "./logo64.png",
        "128": "./logo128.png"
    },
    "background": {
        "scripts": [
            "./background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "https://ltuc-lab.herokuapp.com/*"
            ],
            "js": [
                "./foreground.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "./popup.html"
    },
    "permissions": [
        "https://ltuc-lab.herokuapp.com/*",
        "storage"
    ]
}